gaming_creative INT(1),
gaming_mpp INT(1),
gaming_rest_id INT(1),
gaming_pat INT(1),
gaming_carousel INT(1),
gaming_static_tile INT(1),
gaming_brat_tile INT(1),
gaming_netflix_video_banner INT(1),
poker_creative INT(1),
poker_tournament_page INT(1),
poker_rest_id INT(1),
poker_pat INT(1),
poker_carousel INT(1),
poker_static_tile INT(1),
poker_client_overlay INT(1),
poker_mobile_banner INT(1),
poker_big_banner_exit_banner_exit_banner_cta INT(1),
sports_creative INT(1),
sports_mpp INT(1),
sports_rest_id INT(1),
sports_core_domain INT(1),
sports_static_tile INT(1),
sports_dev_creative INT(1),
sports_dev_landing_page INT(1),
sports_dev_mpp INT(1),
sports_dev_rest_id INT(1),
sports_dev_mai INT(1),
sports_pat INT(1),
gaming_dev_creative INT(1),
gaming_dev_landing_page INT(1),
gaming_dev_mpp INT(1),
gaming_dev_rest_id INT(1),
gaming_dev_pat INT(1),
gaming_dev_layout INT(1),
gaming_dev_carousel INT(1),
poker_dev_creative INT(1),
poker_dev_landing_page INT(1),
poker_dev_tournament_page INT(1),
poker_dev_rest_id INT(1),
poker_dev_pat INT(1),
poker_dev_carousel INT(1),
poker_dev_layout INT(1),
scheduling_mpp_id INT(1),
scheduling_sc_pat_finalizing INT(1),
scheduling_carousel INT(1),
scheduling_static_tile INT(1),
scheduling_tournament_page INT(1),
scheduling_client_overlay INT(1),
scheduling_mobile_banner INT(1),
scheduling_sidebar INT(1),
scheduling_navbar INT(1),
qa_ticket_build_notes INT(1)

);